<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.medication().nil? -%><%== render :template => 'reference', :locals => {name: 'medication', model: model.medication()} %><%- end -%>
    <%- if !model.validityPeriod().nil? -%><%== render :template => 'period', :locals => {name: 'validityPeriod', model: model.validityPeriod()} %><%- end -%>
    <%- if !model.numberOfRepeatsAllowed().nil? -%><%== render :template => 'positiveint', :locals => {name: 'numberOfRepeatsAllowed', model: model.numberOfRepeatsAllowed()} %><%- end -%>
    <%- if !model.quantity().nil? -%><%== render :template => 'quantity', :locals => {name: 'quantity', model: model.quantity()} %><%- end -%>
    <%- if !model.expectedSupplyDuration().nil? -%><%== render :template => 'quantity', :locals => {name: 'expectedSupplyDuration', model: model.expectedSupplyDuration()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
