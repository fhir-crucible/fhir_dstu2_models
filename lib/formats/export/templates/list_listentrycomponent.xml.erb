<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if (!model.flag().nil? && !model.flag.empty?) -%><%- model.flag().each do |element| -%><%== render :template => 'codeableconcept', :locals => {name: 'flag', model: element} %><%- end -%><%- end -%>
    <%- if !model.fhirDeleted().nil? -%><deleted value="<%= model.fhirDeleted() %>"/><%- end -%>
    <%- if !model.date().nil? -%><date value="<%= model.date().to_date.iso8601 %>"/><%- end -%>
    <%- if !model.item().nil? -%><%== render :template => 'reference', :locals => {name: 'item', model: model.item()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
