<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'HumanName' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.use().nil? -%><use value="<%= model.use() %>"/><%- end -%>
    <%- if !model.text().nil? -%><text value="<%= model.text() %>"/><%- end -%>
    <%- if (model.family() && !model.family.empty?) -%><%- model.family().each do |element| -%><family value="<%= element %>"/><%- end -%><%- end -%>
    <%- if (model.given() && !model.given.empty?) -%><%- model.given().each do |element| -%><given value="<%= element %>"/><%- end -%><%- end -%>
    <%- if (model.prefix() && !model.prefix.empty?) -%><%- model.prefix().each do |element| -%><prefix value="<%= element %>"/><%- end -%><%- end -%>
    <%- if (model.suffix() && !model.suffix.empty?) -%><%- model.suffix().each do |element| -%><suffix value="<%= element %>"/><%- end -%><%- end -%>
    <%- if !model.period().nil? -%><%== render :template => 'period', :locals => {name: 'period', model: model.period()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
