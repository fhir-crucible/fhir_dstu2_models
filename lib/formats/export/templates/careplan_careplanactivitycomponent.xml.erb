<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if (model.goal() && !model.goal.empty?) -%><%- model.goal().each do |element| -%><goal value="<%= element %>"/><%- end -%><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status() %>"/><%- end -%>
    <%- if !model.prohibited().nil? -%><prohibited value="<%= model.prohibited() %>"/><%- end -%>
    <%- if (!model.actionResulting().nil? && !model.actionResulting.empty?) -%><%- model.actionResulting().each do |element| -%><%== render :template => 'reference', :locals => {name: 'actionResulting', model: element} %><%- end -%><%- end -%>
    <%- if !model.notes().nil? -%><notes value="<%= model.notes() %>"/><%- end -%>
    <%- if !model.detail().nil? -%><%== render :template => 'reference', :locals => {name: 'detail', model: model.detail()} %><%- end -%>
    <%- if !model.simple().nil? -%><%== render :template => 'careplan_careplanactivitysimplecomponent', :locals => {name: 'simple', model: model.simple()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
