<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.number().nil? -%><number value="<%= model.number() %>"/><%- end -%>
    <%- if !model.uid().nil? -%><uid value="<%= model.uid() %>"/><%- end -%>
    <%- if !model.sopclass().nil? -%><sopclass value="<%= model.sopclass() %>"/><%- end -%>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType() %>"/><%- end -%>
    <%- if !model.title().nil? -%><title value="<%= model.title() %>"/><%- end -%>
    <%- if !model.url().nil? -%><url value="<%= model.url() %>"/><%- end -%>
    <%- if !model.attachment().nil? -%><%== render :template => 'reference', :locals => {name: 'attachment', model: model.attachment()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
