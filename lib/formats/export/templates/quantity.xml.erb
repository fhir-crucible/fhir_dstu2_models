<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Quantity' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model, is_resource: false} %>
    <%- if !model.value().nil? -%><value value="<%= model.value() %>"/><%- end -%>
    <%- if !model.comparator().nil? -%><comparator value="<%= model.comparator() %>"/><%- end -%>
    <%- if !model.units().nil? -%><units value="<%= model.units() %>"/><%- end -%>
    <%- if !model.system().nil? -%><system value="<%= model.system() %>"/><%- end -%>
    <%- if !model.code().nil? -%><code value="<%= model.code() %>"/><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
